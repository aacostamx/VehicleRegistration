<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Inkript.VR.Models"
                   namespace="Inkript.VR.Models">

  <class name="Cars" table="Cars" lazy="true" >
    <id name="Id" column="CarId">
      <generator class="identity" />
    </id>
    <property name="Type">
      <column name="Type" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="CertificateId">
      <column name="CertificateId" sql-type="int" not-null="true" />
    </property>
    <property name="ColorId">
      <column name="ColorId" sql-type="int" not-null="true" />
    </property>
    <property name="Cylinders">
      <column name="Cylinders" sql-type="int" not-null="true" />
    </property>
    <property name="Office">
      <column name="Office" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="YearMake">
      <column name="YearMake" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Value">
      <column name="Value" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Taxes">
      <column name="Taxes" sql-type="int" not-null="true" />
    </property>
    <property name="Discounted">
      <column name="Discounted" sql-type="bit" not-null="true" />
    </property>
    <property name="DateCreated">
      <column name="DateCreated" sql-type="date" not-null="true" />
    </property>
    <property name="DateModified">
      <column name="DateModified" sql-type="date" not-null="true" />
    </property>
    <property name="CreateBy">
      <column name="CreateBy" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="ModifiedBy">
      <column name="ModifiedBy" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CarRegisteredFlag">
      <column name="CarRegisteredFlag" sql-type="bit" not-null="true" />
    </property>
    <property name="ImportId">
      <column name="ImportId" sql-type="int" not-null="true" />
    </property>
    <property name="ImportedName">
      <column name="ImportedName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="MotorNumber">
      <column name="MotorNumber" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Status">
      <column name="Status" sql-type="bit" not-null="true" />
    </property>
  </class>

</hibernate-mapping>
